{
  "name": "@folio/gobi-settings",
  "version": "1.0.1",
  "description": "GOBI integration settings",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/folio-org/ui-gobi-settings.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "yarn stripes serve",
    "build": "yarn stripes build --output ./output",
    "test:unit": "jest --ci --coverage && yarn run test:unit:report",
    "test:unit:report": "cp -r ./artifacts/coverage-jest ./artifacts/coverage",
    "test": "yarn run test:unit",
    "lint": "eslint .",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-gobi-settings ./translations/ui-gobi-settings/compiled",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@folio/eslint-config-stripes": "^6.1.0",
    "@folio/stripes": "^7.0.0",
    "@folio/stripes-cli": "^2.4.0",
    "@formatjs/cli": "^4.2.16",
    "@testing-library/dom": "^7.29.6",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^11.0.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^13.5.0",
    "babel-jest": "^26.3.0",
    "core-js": "^3.6.1",
    "eslint": "^7.32.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jest": "^23.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.4.0",
    "react-intl": "^5.7.1",
    "react-query": "^3.6.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.3"
  },
  "dependencies": {
    "@folio/stripes-acq-components": "~3.3.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.0",
    "query-string": "^7.1.2"
  },
  "peerDependencies": {
    "@folio/stripes": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.7.1",
    "react-router-dom": "^5.2.0"
  },
  "stripes": {
    "actsAs": [
      "settings"
    ],
    "displayName": "ui-gobi-settings.meta.title",
    "queryResource": "query",
    "route": "/gobi-integration",
    "icons": [
      {
        "name": "app",
        "alt": "GOBI integration",
        "title": "GOBI integration"
      }
    ],
    "okapiInterfaces": {
      "custom-mapping": "1.0",
      "users": "15.1 16.0"
    },
    "stripesDeps": [
      "@folio/stripes-acq-components"
    ],
    "permissionSets": [
      {
        "permissionName": "settings.gobi-settings.enabled",
        "displayName": "GOBI Integration: display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-gobi-settings.permission.settings",
        "displayName": "GOBI Integration: view and edit settings",
        "subPermissions": [
          "settings.gobi-settings.enabled",
          "gobi.mappings.translators.collection.get",
          "gobi.mappings.types.collection.get",
          "gobi.custom-mappings.collection.get",
          "gobi.custom-mappings.item.get",
          "gobi.custom-mappings.item.post",
          "gobi.custom-mappings.item.put",
          "gobi.custom-mappings.item.delete"
        ],
        "visible": true
      }
    ]
  }
}
